{
  "name": "seed-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "test": "npm run test:unit",
    "lint": "vue-cli-service lint",
    "start": "npm run serve",
    "test:unit": "jest  --no-cache"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.0",
    "@iti/vue-core": "1.0.3",
    "material-design-icons": "3.0.1",
    "vee-validate": "2.1.3",
    "vue": "2.5.21",
    "vue-class-component": "6.3.2",
    "vue-i18n": "8.10.0",
    "vue-property-decorator": "7.2.0",
    "vue-router": "3.0.2",
    "vuetify": "1.5.14",
    "vuex": "3.1.0",
    "vuex-module-decorators": "0.9.8"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.8.0",
    "@kazupon/vue-i18n-loader": "0.3.0",
    "@types/jest": "23.3.12",
    "@types/jquery": "3.3.29",
    "@types/webpack": "4.4.22",
    "@vue/cli-plugin-typescript": "3.3.0",
    "@vue/cli-service": "3.5.1",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-serializer-vue": "2.0.2",
    "node-sass": "4.13.0",
    "sass-loader": "7.1.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2",
    "vue-cli-plugin-i18n": "2.3.1",
    "vue-cli-plugin-vuetify": "0.4.6",
    "vue-jest": "3.0.2",
    "vue-template-compiler": "2.5.21",
    "vuetify-loader": "1.1.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
